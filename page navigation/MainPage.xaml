<Page x:Class="page_navigation.MainPage" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:local="using:page_navigation"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Background="{ThemeResource ApplicationPageBackgroundThemeBrush}" mc:Ignorable="d">

    <StackPanel Margin="30,0">
        <TextBlock Text="Page 1" />
        <TextBlock x:Name="txtPage1" Text="Hello" />
        <Button x:Name="BtnPage1" Margin="30" Click="BtnPage1_Click" Content="Go to Page 2" />
        <Button x:Name="BackBtn" Background="Purple" Click="BackBtn_Click" Content="Back" />
    </StackPanel>
</Page>
