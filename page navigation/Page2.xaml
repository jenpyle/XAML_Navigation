        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:local="using:page_navigation"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Background="{ThemeResource ApplicationPageBackgroundThemeBrush}" mc:Ignorable="d">

    <StackPanel>
        <TextBlock Text="Page 2" />
        <Button x:Name="BtnPage2" Margin="30" Click="BtnPage2_Click" Content="Go to Data" />
        <Button x:Name="BackBtn2" Click="BackBtn_Click" Content="Back" Background="DeepPink" />
        <TextBlock x:Name="pg2" Text="Page 2" />

    </StackPanel>
</Page>
